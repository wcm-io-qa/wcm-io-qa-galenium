<root>
    <windows>
        <driver id="googlechrome">
            <version id="${galenium.webdriver.download.chrome.version}">
                <bitrate thirtytwobit="true" sixtyfourbit="true">
                    <filelocation>http://chromedriver.storage.googleapis.com/${galenium.webdriver.download.chrome.version}/chromedriver_win32.zip</filelocation>
                    <hash>DO_NOT_CHECK</hash>
                    <hashtype>sha1</hashtype>
                </bitrate>
            </version>
        </driver>
        <driver id="marionette">
            <version id="${galenium.webdriver.download.firefox.version}">
                <bitrate sixtyfourbit="true">
                    <filelocation>https://github.com/mozilla/geckodriver/releases/download/v${galenium.webdriver.download.firefox.version}/geckodriver-v${galenium.webdriver.download.firefox.version}-win64.zip</filelocation>
                    <hash>DO_NOT_CHECK</hash>
                    <hashtype>sha1</hashtype>
                </bitrate>
                <bitrate thirtytwobit="true">
                    <filelocation>https://github.com/mozilla/geckodriver/releases/download/v${galenium.webdriver.download.firefox.version}/geckodriver-v${galenium.webdriver.download.firefox.version}-win32.zip</filelocation>
                    <hash>DO_NOT_CHECK</hash>
                    <hashtype>sha1</hashtype>
                </bitrate>
            </version>
        </driver>
    </windows>
    <linux>
        <driver id="googlechrome">
            <version id="${galenium.webdriver.download.chrome.version}">
                <bitrate sixtyfourbit="true">
                    <filelocation>http://chromedriver.storage.googleapis.com/${galenium.webdriver.download.chrome.version}/chromedriver_linux64.zip</filelocation>
                    <hash>DO_NOT_CHECK</hash>
                    <hashtype>sha1</hashtype>
                </bitrate>
                <bitrate thirtytwobit="true">
                    <filelocation>http://chromedriver.storage.googleapis.com/${galenium.webdriver.download.chrome.version}/chromedriver_linux32.zip</filelocation>
                    <hash>DO_NOT_CHECK</hash>
                    <hashtype>sha1</hashtype>
                </bitrate>
            </version>
        </driver>
        <driver id="marionette">
            <version id="${galenium.webdriver.download.firefox.version}">
                <bitrate sixtyfourbit="true">
                    <filelocation>https://github.com/mozilla/geckodriver/releases/download/v${galenium.webdriver.download.firefox.version}/geckodriver-v${galenium.webdriver.download.firefox.version}-linux64.tar.gz</filelocation>
                    <hash>DO_NOT_CHECK</hash>
                    <hashtype>sha1</hashtype>
                </bitrate>
                <bitrate thirtytwobit="true">
                    <filelocation>https://github.com/mozilla/geckodriver/releases/download/v${galenium.webdriver.download.firefox.version}/geckodriver-v${galenium.webdriver.download.firefox.version}-linux32.tar.gz</filelocation>
                    <hash>DO_NOT_CHECK</hash>
                    <hashtype>sha1</hashtype>
                </bitrate>
            </version>
        </driver>
    </linux>
    <osx>
        <driver id="googlechrome">
            <version id="${galenium.webdriver.download.chrome.version}">
                <bitrate thirtytwobit="true" sixtyfourbit="true">
                    <filelocation>http://chromedriver.storage.googleapis.com/${galenium.webdriver.download.chrome.version}/chromedriver_mac64.zip</filelocation>
                    <hash>DO_NOT_CHECK</hash>
                    <hashtype>sha1</hashtype>
                </bitrate>
            </version>
        </driver>
        <driver id="marionette">
            <version id="${galenium.webdriver.download.firefox.version}">
                <bitrate thirtytwobit="true" sixtyfourbit="true">
                    <filelocation>https://github.com/mozilla/geckodriver/releases/download/v${galenium.webdriver.download.firefox.version}/geckodriver-v${galenium.webdriver.download.firefox.version}-macos.tar.gz</filelocation>
                    <hash>DO_NOT_CHECK</hash>
                    <hashtype>sha1</hashtype>
                </bitrate>
            </version>
        </driver>
    </osx>
</root>
